<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.layout.VBox?>

<AnchorPane
  fx:id="commonBackground"
  prefHeight="745.0"
  prefWidth="702.0"
  stylesheets="@background.css"
  xmlns="http://javafx.com/javafx/16"
  xmlns:fx="http://javafx.com/fxml/1"
>
   <children>
      <VBox
        alignment="TOP_CENTER"
        prefHeight="745.0"
        prefWidth="1097.0"
      >
         <children>
            <AnchorPane
              fx:id="fieldContentAnchor"
              prefHeight="682.0"
              prefWidth="1097.0"
            >
               <children>
                  <Pane
                    fx:id="imagePane"
                    layoutX="549.0"
                    layoutY="341.0"
                    prefHeight="224.0"
                    prefWidth="223.0"
                  >
                     <children>
                        <AnchorPane
                          fx:id="fieldForColony"
                          layoutX="12.0"
                          layoutY="12.0"
                          prefHeight="200.0"
                          prefWidth="200.0"
                        />
                     </children>
                  </Pane>
               </children>
            </AnchorPane>
            <AnchorPane
              prefHeight="58.0"
              prefWidth="1097.0"
            >
               <children>
                  <HBox
                    alignment="CENTER"
                    prefHeight="55.0"
                    prefWidth="1097.0"
                  >
                     <children>
                        <Pane
                          fx:id="buttonsImagedPane"
                          opacity="1" prefHeight="60.0"
                          prefWidth="937.0"
                        >
                           <children>
                              <AnchorPane
                                prefHeight="65.0"
                                prefWidth="1024.0"
                              >
                                 <children>
                                    <Pane
                                      fx:id="miniPane"
                                      layoutX="111.0"
                                      layoutY="10.0"
                                      prefHeight="39.0"
                                      prefWidth="490.0"
                                    >
                                       <children>
                                          <Button
                                            fx:id="startButton"
                                            alignment="CENTER"
                                            layoutX="225.0"
                                            layoutY="7.0"
                                            mnemonicParsing="false"
                                            onAction="#startButtonClick"
                                            styleClass="buttons"
                                            text="Start" />
                                          <Button
                                            fx:id="stopButton"
                                            alignment="CENTER"
                                            layoutX="325.0"
                                            layoutY="7.0"
                                            mnemonicParsing="false"
                                            onAction="#stopButtonClick"
                                            styleClass="buttons"
                                            text="Stop"
                                          />
                                          <Button
                                            fx:id="clearButton"
                                            alignment="CENTER"
                                            layoutX="420.0"
                                            layoutY="7.0"
                                            mnemonicParsing="false"
                                            onAction="#clearButtonClick"
                                            styleClass="buttons"
                                            text="Clear"
                                          />
                                       </children>
                                    </Pane>
                                 </children>
                              </AnchorPane>
                           </children>
                        </Pane>
                     </children>
                  </HBox>
               </children>
            </AnchorPane>
         </children>
      </VBox>
   </children>

</AnchorPane>
